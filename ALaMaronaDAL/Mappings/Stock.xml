<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="ALaMarona.Domain" namespace="ALaMarona.Domain.Entities">
  <class name="Stock">
    <id name="Id">
      <generator class="identity"></generator>
    </id>
    <many-to-one name="Producto" class="ALaMarona.Domain.Entities.Producto, ALaMarona.Domain">
      <column name="idProducto"></column>
    </many-to-one>
    <set cascade="all-delete-orphan" inverse="true" name="_movimientos">
      <key column="idStock"/>
      <one-to-many class="ALaMarona.Domain.Entities.MovimientoStock, ALaMarona.Domain"/>
    </set>
  </class>
</hibernate-mapping>
