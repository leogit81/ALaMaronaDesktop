<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="ALaMarona.Domain" namespace="ALaMarona.Domain.Entities">
  <class name="Direccion">
    <id name="Id">
      <generator class="identity"></generator>
    </id>
    <property name="Calle" not-null="false" />
    <property name="Altura"/>
    <property name="Piso"/>
    <property name="Departamento"/>
    <property name="CodigoPostal" not-null="false" />
    
    <many-to-one name="Pais" class="ALaMarona.Domain.Entities.Pais, ALaMarona.Domain" not-null="false" cascade="all">
      <column name="IdPais"/>
    </many-to-one>
    <many-to-one name="Provincia" class="ALaMarona.Domain.Entities.Provincia, ALaMarona.Domain" not-null="false" cascade="all">
      <column name="IdProvincia"/>
    </many-to-one>
    <many-to-one name="Localidad" class="ALaMarona.Domain.Entities.Localidad, ALaMarona.Domain" cascade="all" >
      <column name="IdLocalidad"/>
    </many-to-one>
  </class>
</hibernate-mapping>
