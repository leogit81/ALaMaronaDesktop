<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="ALaMarona.Domain" namespace="ALaMarona.Domain.Entities">
  <class name="MovimientoStock" table="StockMovimiento">
    <id name="Id">
      <generator class="identity"></generator>
    </id>
    <property name="Fecha" not-null="true" />
    <property name="Cantidad" not-null="true" />
    <property name="PrecioUnitario" not-null="true" />
    <many-to-one name="Pais" class="ALaMarona.Domain.Entities.Stock, ALaMarona.Domain" >
      <column name="idStock"/>
    </many-to-one>
  </class>
</hibernate-mapping>
